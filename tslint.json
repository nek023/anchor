{
  "defaultSeverity": "error",
  "extends": [],
  "jsRules": {},
  "rules": {
    "arrow-parens": [true, "ban-single-arg-parens"],
    "arrow-return-shorthand": true,
    "indent": [true, "spaces", 2],
    "ordered-imports": true,
    "quotemark": [true, "single"],
    "semicolon": [true, "never"],
    "trailing-comma": [
      true,
      {
        "esSpecCompliant": true,
        "multiline": {
          "arrays": "always",
          "objects": "always"
        },
        "singleline": "never"
      }
    ]
  },
  "rulesDirectory": []
}
