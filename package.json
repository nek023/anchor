{
  "name": "anchor",
  "version": "0.1.0",
  "description": "Tab switcher for Google Chrome",
  "main": "build/index.js",
  "scripts": {
    "clean": "rm -rf build",
    "build": "webpack --progress -d",
    "build:watch": "webpack --progress -d --watch",
    "build:dist": "webpack --progress -p",
    "package": "npm run clean && npm run build:dist && zip package.zip -r build icons index.html manifest.json",
    "test": "mocha --require test/setup.js --compilers js:babel-register --recursive",
    "test:watch": "npm test -- --watch",
    "test:lint": "eslint test; exit 0"
  },
  "author": "Katsuma Tanaka",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.7.13",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-espower": "^2.3.1",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.11.6",
    "css-loader": "^0.23.1",
    "enzyme": "^2.4.1",
    "eslint": "^3.2.2",
    "eslint-loader": "^1.5.0",
    "eslint-plugin-react": "^6.0.0",
    "faker": "^3.1.0",
    "jsdom": "^9.4.2",
    "mocha": "^3.0.2",
    "node-sass": "^3.8.0",
    "power-assert": "^1.4.1",
    "react-addons-perf": "^15.3.0",
    "react-addons-test-utils": "^15.3.0",
    "redux-logger": "^2.6.1",
    "sass-loader": "^4.0.0",
    "sinon": "^1.17.5",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "babel-polyfill": "^6.13.0",
    "fuse.js": "^2.4.1",
    "immutable": "^3.8.1",
    "react": "^15.3.0",
    "react-dom": "^15.3.0",
    "react-redux": "^4.4.5",
    "redux": "^3.5.2",
    "redux-actions": "^0.10.1",
    "redux-saga": "^0.11.0"
  }
}
