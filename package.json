{
  "name": "anchor",
  "version": "1.0.1",
  "description": "Tab switcher for Google Chrome",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf dist",
    "build": "webpack --debug --output-pathinfo --colors --progress",
    "build:watch": "webpack --debug --output-pathinfo --colors --progress --watch",
    "build:dist": "NODE_ENV=production webpack -p --colors --progress",
    "lint": "eslint src; exit 0",
    "test": "mocha -r babel-register -r test/setup.js --recursive",
    "test:watch": "npm test -- --watch",
    "test:lint": "eslint test; exit 0",
    "package": "npm run clean && npm run build:dist && zip package.zip -r dist icons index.html manifest.json",
    "storybook": "start-storybook -s stories/static -p 3000",
    "storybook:lint": "eslint storybook; exit 0",
    "build-storybook": "build-storybook"
  },
  "author": "Katsuma Tanaka",
  "license": "MIT",
  "dependencies": {
    "@storybook/cli": "^3.2.4",
    "@storybook/react": "^3.2.4",
    "babel-core": "^6.14.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^7.1.1",
    "babel-polyfill": "^6.20.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-power-assert": "^1.0.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.14.0",
    "css-loader": "^0.28.4",
    "enzyme": "^2.4.1",
    "eslint": "^4.4.1",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-react": "^7.2.0",
    "faker": "^4.1.0",
    "fuse.js": "^3.0.5",
    "immutable": "^3.8.1",
    "jsdom": "^11.1.0",
    "mocha": "^3.5.0",
    "node-sass": "^4.1.1",
    "power-assert": "^1.4.1",
    "prop-types": "^15.5.10",
    "react": "^15.4.1",
    "react-addons-perf": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-immutable-proptypes": "^2.1.0",
    "react-redux": "^5.0.6",
    "react-test-renderer": "^15.6.1",
    "redux": "^3.7.2",
    "redux-actions": "^2.2.1",
    "redux-logger": "^3.0.6",
    "redux-saga": "^0.15.6",
    "rimraf": "^2.5.4",
    "sass-loader": "^6.0.6",
    "sinon": "^3.2.0",
    "style-loader": "^0.18.2",
    "webpack": "^3.5.4",
    "webpack-dev-server": "^2.7.1"
  }
}
