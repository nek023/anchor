{
  "name": "anchor",
  "version": "0.1.0",
  "description": "Tab switcher for Google Chrome",
  "main": "build/index.js",
  "scripts": {
    "clean": "rimraf build",
    "build": "webpack --progress -d",
    "build:watch": "webpack --progress -d --watch",
    "build:dist": "NODE_ENV=production webpack --progress -p",
    "lint": "eslint src; exit 0",
    "test": "mocha --require test/setup.js --compilers js:babel-register --recursive",
    "test:watch": "npm test -- --watch",
    "test:lint": "eslint test; exit 0",
    "package": "npm run clean && npm run build:dist && zip package.zip -r build icons index.html manifest.json",
    "storybook": "start-storybook -s storybook/static -c storybook -p 3000",
    "storybook:lint": "eslint storybook; exit 0"
  },
  "author": "Katsuma Tanaka",
  "license": "MIT",
  "devDependencies": {
    "@kadira/storybook": "^2.16.1",
    "babel-core": "^6.14.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.5",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-espower": "^2.3.1",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.14.0",
    "css-loader": "^0.26.1",
    "enzyme": "^2.4.1",
    "eslint": "^3.5.0",
    "eslint-loader": "^1.5.0",
    "eslint-plugin-react": "^6.3.0",
    "faker": "^3.1.0",
    "jsdom": "^9.5.0",
    "mocha": "^3.0.2",
    "node-sass": "^4.1.1",
    "power-assert": "^1.4.1",
    "react-addons-perf": "^15.4.1",
    "react-addons-test-utils": "^15.4.1",
    "redux-logger": "^2.6.1",
    "rimraf": "^2.5.4",
    "sass-loader": "^4.0.2",
    "sinon": "^1.17.6",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.16.1"
  },
  "dependencies": {
    "babel-polyfill": "^6.20.0",
    "fuse.js": "^2.6.1",
    "immutable": "^3.8.1",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-redux": "^5.0.1",
    "redux": "^3.6.0",
    "redux-saga": "^0.14.0"
  }
}
